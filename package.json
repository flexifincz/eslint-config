{
  "name": "@flexifincz/eslint-config",
  "version": "1.0.8",
  "description": "Code service level repo for sharing common sources through several FF projects.",
  "repository": "flexifincz/eslint-config",
  "license": "UNLICENSED",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./prettier": {
      "types": "./dist/prettier.d.ts",
      "import": "./dist/prettier.mjs",
      "require": "./dist/prettier.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "test": "pnpm lint && pnpm test:types",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "automd": "^0.3.8",
    "changelogen": "^0.5.5",
    "eslint": "^9.8.0",
    "eslint-config-unjs": "^0.3.2",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "unbuild": "^3.0.0-rc.7"
  },
  "packageManager": "pnpm@9.7.0",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^55.0.0"
  },
  "publishConfig": {
    "@flexifincz:registry": "https://npm.pkg.github.com"
  }
}
